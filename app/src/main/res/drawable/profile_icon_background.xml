    <!-- Ikon orang di tengah -->
    <item
        android:drawable="@android:drawable/ic_menu_myplaces"
        android:width="20dp"
        android:height="20dp"
        android:gravity="center"
        android:tint="#FFFFFF" /> <!-- Warna ikon putih -->
</layer-list>