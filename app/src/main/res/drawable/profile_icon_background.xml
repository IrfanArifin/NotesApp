<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Latar belakang lingkaran biru -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#007AFF" /> <!-- Biru iOS -->
            <size
                android:width="32dp"
                android:height="32dp" />
